{
  "name": "tocaan/dukan",
  "description": "Multi-tenant helper services for Laravel using Stancl Tenancy.",
  "type": "library",
  "license": "MIT",
  "keywords": ["Laravel", "Multi-tenant", "S3", "Cloudflare", "Tenancy", "Stancl","Ploi"],
  "require": {
    "guzzlehttp/guzzle": "*",
    "illuminate/bus": "*",
    "illuminate/queue": "*",
    "illuminate/support": "*",
    "illuminate/events": "*",
    "php": "^8.0",
    "stancl/tenancy": "^3.8",
    "aws/aws-sdk-php": "^3.342"
  },
  "authors": [
    {
      "name": "eslamshatta1",
      "email": "eslam.shatta1@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Tocaan\\Dukan\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tocaan\\Dukan\\DukanServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
